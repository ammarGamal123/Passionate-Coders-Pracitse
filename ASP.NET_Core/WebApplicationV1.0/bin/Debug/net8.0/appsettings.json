{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=.;database=Products; integrated security = true; trust server certificate=true"
  },
  "Attachments": {
    "AllowedExtensions": "jpg;jpeg;bmp;png",
    "MaxSizeInMegaBytes": 1,
    "EnableCompression": true
  }
}
